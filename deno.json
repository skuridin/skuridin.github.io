{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/,features/ dev.ts"
  },
  "importMap": "./import_map.json",
  "fmt": {
    "files": {
      "include": [
        "routes",
        "features"
      ]
    },
    "options": {
      "useTabs": true
    }
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}